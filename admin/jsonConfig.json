{
    "i18n": true,
    "type": "tabs",
    "tabsStyle": {
        "width": "calc(100% - 100px)"
    },
    "items": {
        "_options": {
            "type": "panel",
            "label": "Options",
            "items": {
                "impUnitPerImpulse": {
                    "type": "number",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "xl": 4,
                    "label": "Units per impulse (default)",
                    "help": "One impulse is equal to this value"
                },
                "impFactor": {
                    "type": "number",
                    "step": 0.001,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "xl": 4,
                    "label": "Group factor (default)",
                    "help": "Values will be added to groups with this factor"
                },
                "timezone": {
                    "newLine": true,
                    "type": "text",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "xl": 4,
                    "label": "Time zone",
                    "help": "e.g. Europe/Berlin (leave empty for default settings)"
                }
            }
        },
        "_groups": {
            "type": "panel",
            "label": "Groups",
            "items": {
                "groups": {
                    "type": "table",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12,
                    "label": "Groups",
                    "items": [
                        {
                            "type": "text",
                            "attr": "id",
                            "width": "25%",
                            "title": {
                                "en": "ID",
                                "de": "ID",
                                "ru": "КОД",
                                "pt": "ID",
                                "nl": "ID",
                                "fr": "ID",
                                "it": "ID",
                                "es": "ID",
                                "pl": "ID",
                                "zh-cn": "ID"
                            },
                            "filter": true,
                            "sort": true,
                            "default": ""
                        },
                        {
                            "type": "text",
                            "attr": "name",
                            "width": "35%",
                            "title": {
                                "en": "Name",
                                "de": "Name",
                                "ru": "Имя",
                                "pt": "Nome",
                                "nl": "Naam",
                                "fr": "Nom",
                                "it": "Nome",
                                "es": "Nombre",
                                "pl": "Nazwa",
                                "zh-cn": "姓名"
                            },
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "number",
                            "attr": "price",
                            "width": "20%",
                            "title": {
                                "en": "Price",
                                "de": "Preis",
                                "ru": "Цена",
                                "pt": "Preço",
                                "nl": "Price",
                                "fr": "Prix",
                                "it": "Prezzo",
                                "es": "Precio",
                                "pl": "Price",
                                "zh-cn": "价格"
                            },
                            "filter": false,
                            "sort": false,
                            "default": false
                        },
                        {
                            "type": "text",
                            "attr": "priceUnit",
                            "width": "20%",
                            "title": {
                                "en": "Unit",
                                "de": "Einheit",
                                "ru": "Единица",
                                "pt": "Unidade",
                                "nl": "Unit",
                                "fr": "Unité",
                                "it": "Unità",
                                "es": "Dependencia",
                                "pl": "Jednostka",
                                "zh-cn": "股"
                            },
                            "filter": false,
                            "sort": false,
                            "default": "€/kWh"
                        }
                    ]
                }
            }
        }
    }
}